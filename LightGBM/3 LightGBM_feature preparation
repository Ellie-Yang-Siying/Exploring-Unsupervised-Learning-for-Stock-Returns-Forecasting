ry.intrinsic+json":{"type":"dataframe","variable_name":"filtered_df_orig"}},"metadata":{},"execution_count":6}]},{"cell_type":"markdown","source":["# Feature Engineering"],"metadata":{"id":"WkMik8h08wGm"}},{"cell_type":"code","source":["def generate_features(data, lags=5, rolling_windows=[5, 10, 20]):\n","    \"\"\"\n","    Generate features for each stock, including lag features, rolling statistics, and technical indicators.\n","\n","    Parameters:\n","    - data: Original data, a DataFrame where columns are stock symbols and rows are indexed by dates.\n","    - lags: Number of lag days to include as features.\n","    - rolling_windows: List of rolling window sizes for calculating statistics.\n","\n","    Returns:\n","    - features: A DataFrame of generated features.\n","    \"\"\"\n","    features = pd.DataFrame(index=data.index)\n","\n","    for stock in data.columns:\n","        # Add lag features\n","        for lag in range(1, lags + 1):\n","            features[f'{stock}_Lag_{lag}'] = data[stock].shift(lag)\n","\n","        # Add rolling statistics features\n","        for window in rolling_windows:\n","            features[f'{stock}_MA_{window}'] = data[stock].rolling(window).mean()\n","            features[f'{stock}_STD_{window}'] = data[stock].rolling(window).std()\n","            features[f'{stock}_Max_{window}'] = data[stock].rolling(window).max()\n","            features[f'{stock}_Min_{window}'] = data[stock].rolling(window).min()\n","\n","        # Add rate of change (ROC)\n","        features[f'{stock}_ROC'] = data[stock].pct_change(periods=1)\n","\n","        # Add Relative Strength Index (RSI)\n","        delta = data[stock].diff(1)\n","        gain = (delta.where(delta > 0, 0)).rolling(window=14).mean()\n","        loss = (-delta.where(delta < 0, 0)).rolling(window=14).mean()\n","        rs = gain / loss\n","        features[f'{stock}_RSI'] = 100 - (100 / (1 + rs))\n","\n","    # Add time-related features\n","    features['Year'] = data.index.year\n","    features['Month'] = data.index.month\n","    features['Day'] = data.index.day\n","    features['Weekday'] = data.index.weekday\n","\n","    # Drop rows with NaN values caused by feature generation\n","    features = features.dropna()\n","\n","    return features"],"metadata":{"id":"Yzdpdl3YFxUp"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["filtered_df_train = filtered_df_orig[(filtered_df_orig.index >= \"2017-01-01\") & (filtered_df_orig.index <= \"2021-12-31\")]\n","filtered_df_test  = filtered_df_orig[(filtered_df_orig.index >= \"2022-01-01\") & (filtered_df_orig.index <= \"2022-12-31\")]"],"metadata":{"id":"_vS3L2egAhkq"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["features_df_train = generate_features(filtered_df_train)\n","features_df_test  = generate_features(filtered_df_test)"],"metadata":{"collapsed":true,"id":"Cmqc3ZsUF0cz"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["filtered_df_orig"],"metadata":{"id":"zqiaVHq1BJ_T","executionInfo":{"status":"ok","timestamp":1733606816970,"user_tz":300,"elapsed":235,"user":{"displayName":"ismail berbache","userId":"08724042472302591562"}},"outputId":"b32ca6fa-a53d-471c-da69-81668ce68b8a","colab":{"base_uri":"https://localhost:8080/","height":649}},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                                  3IN         FOUR        ABDN         ALW  \\\n","Date                                                                         \n","2017-01-03 00:00:00+00:00  193.466919  1506.214478  236.336594  633.503723   \n","2017-01-04 00:00:00+00:00  193.052505  1558.152954  236.024658  638.468445   \n","2017-01-05 00:00:00+00:00  193.778107  1511.240845  226.603668  641.447449   \n","2017-01-06 00:00:00+00:00  193.674942  1528.833008  227.102783  644.426208   \n","2017-01-09 00:00:00+00:00  193.466919  1548.100342  224.919144  652.866333   \n","...                               ...      